	};

	nau8822_sound: nau8822-sound {
		status = "okay";
		compatible = "rockchip,multicodecs-card";
		rockchip,card-name = "rockchip-nau8822";
		hp-det-gpio = <&gpio1 RK_PB2 GPIO_ACTIVE_HIGH>;
		io-channels = <&saradc 3>;
		io-channel-names = "adc-detect";
		keyup-threshold-microvolt = <1800000>;
		poll-interval = <100>;
//		spk-con-gpio = <&gpio1 RK_PD3 GPIO_ACTIVE_HIGH>;
//		hp-con-gpio = <&gpio1 RK_PD2 GPIO_ACTIVE_HIGH>;
		rockchip,format = "i2s";
		rockchip,mclk-fs = <256>;
		rockchip,cpu = <&i2s0_8ch>;
		rockchip,codec = <&nau8822>;
		rockchip,audio-routing =
			"Headphone", "RHP",
			"Headphone", "LHP",
			"Speaker", "LSPK",
			"Speaker", "RSPK",
			"RMICP", "Main Mic",
			"RMICN", "Main Mic",
			"RMICP", "Headset Mic",
			"RMICN", "Headset Mic",
			"LMICP", "Main Mic",
			"LMICN", "Main Mic",
			"LMICP", "Headset Mic",
			"LMICN", "Headset Mic";
		pinctrl-names = "default";
		pinctrl-0 = <&hp_det>;
		play-pause-key {
			label = "playpause";
			linux,code = <KEY_PLAYPAUSE>;
			press-threshold-microvolt = <2000>;
			poll-interval = <1000>;
		};
	};

	hdmi0_sound: hdmi0-sound {
		status = "okay";
		compatible = "rockchip,hdmi";
		rockchip,mclk-fs = <128>;
		rockchip,card-name = "rockchip-hdmi0";
		rockchip,cpu = <&i2s5_8ch>;
		rockchip,codec = <&hdmi0>;
		rockchip,jack-det;
	};

	dp0_sound: dp0-sound {
		status = "okay";
		compatible = "rockchip,hdmi";
		rockchip,card-name= "rockchip,dp0";
		rockchip,mclk-fs = <512>;
		rockchip,cpu = <&spdif_tx2>;
		rockchip,codec = <&dp0 1>;
		rockchip,jack-det;
	};

	dp1_sound: dp1-sound {
		status = "okay";
		compatible = "rockchip,hdmi";
		rockchip,card-name= "rockchip,dp1";
		rockchip,mclk-fs = <512>;
		rockchip,cpu = <&spdif_tx5>;
		rockchip,codec = <&dp1 1>;
		rockchip,jack-det;
	};

	test-power {
		status = "okay";
	};
	dm542_x:dm542-x{
		compatible = "dm542";
		status = "okay";
		ena{
			gpios = <&gpio3 RK_PA4 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&dm542_x_ena>;
		};
		dir{
			gpios = <&gpio3 RK_PA0 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&dm542_x_dir>;
		};
		pul{
			pwms = <&pwm9 0 50000 0>;
		};
		sw1{
			gpios = <&gpio3 RK_PB3 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&dm542_x_sw1>;
			//interrupt-parent = <&gpio3>;
			//interrupts = <RK_PB3 IRQ_TYPE_LEVEL_LOW>;
		};
		sw2{
			gpios = <&gpio3 RK_PB5 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&dm542_x_sw2>;
			//interrupt-parent = <&gpio3>;
			//interrupts = <RK_PB5 IRQ_TYPE_LEVEL_LOW>;
		};
	};
	dm542_y:dm542-y{
		compatible = "dm542";
		status = "okay";
		ena{
			gpios = <&gpio3 RK_PA3 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&dm542_y_ena>;
		};
		dir{
			gpios = <&gpio3 RK_PA7 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&dm542_y_dir>;
		};
		pul{
			pwms = <&pwm15 0 50000 0>;
		};
		sw1{
			gpios = <&gpio3 RK_PB4 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&dm542_y_sw1>;
			//interrupt-parent = <&gpio3>;
			//interrupts = <RK_PB4 IRQ_TYPE_LEVEL_LOW>;
		};
		sw2{
			gpios = <&gpio3 RK_PA2 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&dm542_y_sw2>;
			//interrupt-parent = <&gpio3>;
			//interrupts = <RK_PA2 IRQ_TYPE_LEVEL_LOW>;
		};
	};
	dm542_z:dm542-z{
		compatible = "dm542";
		status = "okay";
		ena{
			gpios = <&gpio3 RK_PB1 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&dm542_z_ena>;
		};
		dir{
			gpios = <&gpio3 RK_PA5 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&dm542_z_dir>;
		};
		pul{
			pwms = <&pwm10 0 50000 0>;
		};
		sw1{
			gpios = <&gpio3 RK_PB6 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&dm542_z_sw1>;
			//interrupt-parent = <&gpio3>;
			//interrupts = <RK_PB6 IRQ_TYPE_LEVEL_LOW>;
		};
		sw2{
			gpios = <&gpio3 RK_PA6 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&dm542_z_sw2>;
			//interrupt-parent = <&gpio3>;
			//interrupts = <RK_PA6 IRQ_TYPE_LEVEL_LOW>;
		};
	};
};
&pwm9 {
	pinctrl-0 = <&pwm9m0_pins>;
	status = "okay";
};
&pwm15{
	pinctrl-0 = <&pwm15m0_pins>;
	status = "okay";
};
&pwm10 {
	pinctrl-0 = <&pwm10m2_pins>;
	status = "okay";
};
&spdif_tx2 {
	status = "okay";
};

&spdif_tx5 {
	status = "okay";
};

&pinctrl {
	dm542{
		dm542_x_ena: dm542-x-ena {
			rockchip,pins = <3 RK_PA4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		dm542_x_dir: dm542-x-dir {
			rockchip,pins = <3 RK_PA0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		dm542_x_sw1: dm542-x-sw1 {
			rockchip,pins = <3 RK_PB3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		dm542_x_sw2: dm542-x-sw2 {
			rockchip,pins = <3 RK_PB5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		dm542_y_ena: dm542-y-ena {
			rockchip,pins = <3 RK_PA3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		dm542_y_dir:dm542-y-dir {
			rockchip,pins = <3 RK_PA7 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		dm542_y_sw1:dm542-y-sw1 {
			rockchip,pins = <3 RK_PB4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		dm542_y_sw2:dm542-y-sw2 {
			rockchip,pins = <3 RK_PA2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		dm542_z_ena: dm542-z-ena {
			rockchip,pins = <3 RK_PB1 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		dm542_z_dir:dm542-z-dir {
			rockchip,pins = <3 RK_PA5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		dm542_z_sw1:dm542-z-sw1 {
			rockchip,pins = <3 RK_PB6 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		dm542_z_sw2:dm542-z-sw2 {
			rockchip,pins = <3 RK_PA6 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	bt {
		bluetooth_wake_pin: bluetooth-wake-pin {
			rockchip,pins = <1 RK_PA4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
	tf {
		sdmmc_pwren_gpio: sdmmc-pwren-gpio {
			rockchip,pins = <2 RK_PC4 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	lcd {
		mipi0_enable_gpio: mipi0-enable-gpio {
			rockchip,pins = <3 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		mipi1_enable_gpio: mipi1-enable-gpio {
			rockchip,pins = <3 RK_PC6 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	tp_int {
		ft5x06_dsi1_gpio:  ft5x06-dsi1-gpio {
			rockchip,pins = <3 RK_PC0 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		gt911_dsi1_gpio:gt911-dsi0-gpio {
			rockchip,pins = <3 RK_PB7 RK_FUNC_GPIO &pcfg_pull_none>,
							<3 RK_PC0 RK_FUNC_GPIO &pcfg_pull_none>;
		};

	};

	hdmirxdet {
		hdmirx_det_gpio: hdmirx-det-gpio {
			rockchip,pins = <1 RK_PD5 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};


	usb {
		vcc5v0_host_en: vcc5v0-host-en {
			rockchip,pins = <4 RK_PB0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	usb-typec {
		usbc0_int: usbc0-int {
			rockchip,pins = <1 RK_PB0 RK_FUNC_GPIO &pcfg_pull_up>;
		};

		typec5v_pwren0: typec5v-pwren0 {
			rockchip,pins = <0 RK_PD0 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		usbc1_int: usbc1-int {
			rockchip,pins = <1 RK_PB3 RK_FUNC_GPIO &pcfg_pull_up>;
		};

		typec5v_pwren1: typec5v-pwren1 {
			rockchip,pins = <0 RK_PD3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	headphone {
		hp_det: hp-det {
			rockchip,pins = <1 RK_PB2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	cam1 {
                cam1_reset_gpio: cam1-reset-gpio {
                        rockchip,pins = <1 RK_PC6 RK_FUNC_GPIO &pcfg_pull_none>;
                };

                cam1_pwren_gpio: cam1-pwren-gpio {
                        rockchip,pins = <1 RK_PD3 RK_FUNC_GPIO &pcfg_pull_none>;
                };
        };
};

&uart4 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart4m0_xfer>;
	status = "disabled";
};


&uart9 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart9m2_xfer>;
	status = "okay";
};

&can1 {
	pinctrl-0 = <&can1m1_pins>;
	status = "disabled";
	assigned-clocks = <&cru CLK_CAN1>;
	assigned-clock-rates = <200000000>;
};

&can2 {
	status = "disabled";
	assigned-clocks = <&cru CLK_CAN2>;
	assigned-clock-rates = <200000000>;
};

&dp0 {
	status = "okay";
};

&dp0_in_vp2 {
	status = "okay";
};

&mdio0 {
	rgmii_phy0: phy@1 {
		compatible = "ethernet-phy-ieee802.3-c22";
		reg = <0x1>;
	};
};


&gmac0 {
	/* Use rgmii-rxid mode to disable rx delay inside Soc */
	phy-mode = "rgmii-rxid";
	clock_in_out = "output";

	snps,reset-gpio = <&gpio0 RK_PB0 GPIO_ACTIVE_LOW>;
	snps,reset-active-low;
	/* Reset time is 20ms, 100ms for rtl8211f */
	snps,reset-delays-us = <0 20000 100000>;

	pinctrl-names = "default";
	pinctrl-0 = <&gmac0_miim
		     &gmac0_tx_bus2
		     &gmac0_rx_bus2
		     &gmac0_rgmii_clk
		     &gmac0_rgmii_bus>;

	tx_delay = <0x44>;
	/* rx_delay = <0x4f>; */

	phy-handle = <&rgmii_phy0>;
	status = "okay";
};


&i2c2 {
	status = "okay";

	ft5x06_dsi0: ft5x06@38 { //dsi0 tp
		compatible = "edt,edt-ft5406", "edt,edt-ft5x06";
		reg = <0x38>;
		pinctrl-names = "ft5x06_defaults";
		pinctrl-0 = <&ft5x06_dsi1_gpio>;
//		interrupt-parent = <&gpio3>;
//		interrupts = <RK_PC0 IRQ_TYPE_EDGE_FALLING>;
        irq-gpio = <&gpio3 RK_PC0 GPIO_ACTIVE_HIGH>;
		touchscreen-size-x = <1024>;
		touchscreen-size-y = <600>;
		status = "okay";
	};

	gt9xx_dsi0: gt9xx@14 {
        compatible = "goodix,gt911";
        reg = <0x14>;
        pinctrl-names = "gt9xx_default";
        pinctrl-0 = <&gt911_dsi1_gpio>;
        //interrupt-parent = <&gpio3>;
        //interrupts = <RK_PC0 IRQ_TYPE_EDGE_FALLING>;
        irq-gpio = <&gpio3 RK_PC0 GPIO_ACTIVE_HIGH>;
        reset-gpio = <&gpio3 RK_PB7 GPIO_ACTIVE_HIGH>;
        touchscreen-size-x = <1024>;
        touchscreen-size-y = <600>;
		is-mutex;
		filter-reg = <0x38>;
		bus-reg = <0x02>;
        status = "okay";
    };

	usbc0: fusb302@22 {
		compatible = "fcs,fusb302";
		reg = <0x22>;
		interrupt-parent = <&gpio1>;
		interrupts = <RK_PB0 IRQ_TYPE_LEVEL_LOW>;
		pinctrl-names = "default";
		pinctrl-0 = <&usbc0_int>;
		vbus-supply = <&vbus5v0_typec0>;
		status = "okay";

		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;
				usbc0_role_sw: endpoint@0 {
					remote-endpoint = <&dwc3_0_role_switch>;
				};
			};
		};

		usb_con0: connector {
			compatible = "usb-c-connector";
			label = "USB-C";
			data-role = "dual";
			power-role = "dual";
			try-power-role = "sink";
			op-sink-microwatt = <1000000>;
			sink-pdos =
				<PDO_FIXED(5000, 1000, PDO_FIXED_USB_COMM)>;
			source-pdos =
				<PDO_FIXED(5000, 3000, PDO_FIXED_USB_COMM)>;

			altmodes {
				#address-cells = <1>;
				#size-cells = <0>;

				altmode@0 {
					reg = <0>;
					svid = <0xff01>;
					vdo = <0xffffffff>;
				};
			};

			ports {
				#address-cells = <1>;
				#size-cells = <0>;

				port@0 {
					reg = <0>;
					usbc0_orien_sw: endpoint {
						remote-endpoint = <&usbdp_phy0_orientation_switch>;
					};
				};

				port@1 {
					reg = <1>;
					dp0_altmode_mux: endpoint {
						remote-endpoint = <&usbdp_phy0_dp_altmode_mux>;
					};
				};
			};
		};
	};

};


